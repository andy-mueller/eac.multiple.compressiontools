<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="compression-tools-section" 
             type="eac.multiple.compressiontools.configuration.CompressionToolConfigurationSection, eac.multiple.compressiontools" />
  </configSections>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>

  
  <compression-tools-section>
    <compression-tools>
      <compression-tool
        executable="program 1"
        parameters="%key% parameters" 
        extension ="xox"/>
      <compression-tool
        executable="program 2"
        parameters="%key2% parameters" 
        extension ="lol"/>
    </compression-tools>
  </compression-tools-section>
</configuration>